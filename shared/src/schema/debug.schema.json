{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DebugReport",
  "type": "object",
  "properties": {
    "originalErrors": {
      "type": "array",
      "items": { "$ref": "issue.schema.json" }
    },
    "testResults": {
      "type": "array",
      "items": { "$ref": "tester.schema.json" }
    }
  },
  "required": ["originalErrors", "testResults"]
}